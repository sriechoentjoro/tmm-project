    /**
     * Index method
     *
     * @return \Cake\Http\Response|null
     */
    public function index()
    {
{% set belongsTo = Bake.aliasExtractor(modelObj, 'BelongsTo') %}
{% if belongsTo %}
        $this->paginate = [
            'contain' => [{{ Bake.stringifyList(belongsTo, {'indent': false})|raw }}],
        ];
{% endif %}
        ${{ pluralName }} = $this->paginate($this->{{ currentModelName }});

{% if belongsTo %}
        // Load dropdown data for filters
{% for assoc in belongsTo %}
        ${{ assoc|lower }} = $this->{{ currentModelName }}->{{ assoc }}->find('list')->limit(200)->toArray();
{% endfor %}
        $this->set(compact('{{ pluralName }}', {{ belongsTo|map(a => "'" ~ a|lower ~ "'")|join(', ')|raw }}));
{% else %}
        $this->set(compact('{{ pluralName }}'));
{% endif %}
    }
